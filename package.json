{
  "name": "nova-colors",
  "version": "2.1.5",
  "scripts": {
    "start": "scripts/start",
    "test": "scripts/test",
    "build": "scripts/build",
    "prepublish": "npm run build && rm -rf src",
    "postpublish": "git checkout -- .",
    "postinstall": "node -e \"require('fs').stat('lib', function (e, s) { process.exit(e || !s.isDirectory() ? 1 : 0) })\" || npm run build"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-jest": "15.0.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "eslint": "3.4.0",
    "jest-cli": "15.1.1",
    "watch": "1.0.1"
  },
  "main": "build/index.js",
  "description": "Single source of truth to consume Nova color values",
  "homepage": "https://trevordmiller.github.io/nova",
  "author": "Trevor D. Miller <trevordmiller@icloud.com> (http://www.trevordmiller.com)",
  "license": "MIT",
  "keywords": [
    "color-scheme",
    "colors"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevordmiller/nova-colors.git"
  }
}
